<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>SOCCER HOUSE</title>
<meta name="keywords" content="" />
<meta name="description" content="" />
<link href="/resources/css/default.css" rel="stylesheet" type="text/css" media="all" />
<link href="/vendors/css/fonts.css" rel="stylesheet" type="text/css" media="all" />
<!--[if IE 6]>
<link href="default_ie6.css" rel="stylesheet" type="text/css" />
<![endif]-->
</head>
<body>
<div id="wrapper">
	<div id="header-wrapper">
		<div id="menu" class="container">
			<ul>
				<li class="active"><a href="#">Home</a></li>
				<li><a href="#">Rosters</a></li>
				<li><a href="#">About</a></li>
			</ul>
		</div>
		<!-- end #menu -->
		<div id="header" class="container">
			<div id="logo">
				<h1><a href="#">SOCCER HOUSE</a></h1>
				<p class="triangle-isosceles">NEXT GAME <a href="http://templated.co" rel="nofollow">BELOW</a></p>
			{{with .matchSchResults}}
				{{$firstRecord := index . 0}}
				<div>
					{{$firstRecord.TeamAway}} VS {{$firstRecord.TeamHome}}
					<p>Date: {{$firstRecord.Date}}</p>
				</div>
			{{else}}
				<div>NO NEXT GAMES SCHEDULED</div>
			{{end}}
			</div>
		</div>
	</div>
	<!--<div id="banner" class="container"> <a href="#" class="image"><img src="images/pic01.jpg" alt="" /></a> </div>-->
	<div id="page" class="container">
		<div id="content">
			<p>SCORES</p>
			<table style="width:100%;" border="1px">
				<tr>
				<th>Team Away</th>
				<th>Score</th>
				<th>Team Home</th>
				<th>Score</th>
				<th>Date</th>
				</tr>
			{{range .scoreResults}}
				<tr>
				<td>{{.TeamAway}}</td>
				<td>{{.TeamAwayScore}}</td>
				<td>{{.TeamHome}}</td>
				<td>{{.TeamHomeScore}}</td>
				<td>{{.Date}}</td>
				</tr>
			{{else}}
				<tr><td colspan="5">No scores to display</td></tr>
			{{end}}
			</table>
		</div>
	</div>
</div>
</div>
<div id="footer" class="container">
	<div>
		<header class="title">
			<h2>Contact Me</h2>
			<span class="byline">I can be reached at ws73@njit.edu</span>
		</header>
	</div>
</div>
<div id="copyright">
	<p>&copy; Soccer House. All rights reserved.</p>
</div>
</body>
</html>
